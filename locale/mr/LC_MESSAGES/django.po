#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid ""
"File extension “%(extension)s” is not allowed. Allowed extensions are: "
"%(allowed_extensions)s."
msgstr ""
"फाइल विस्तार “%(विस्तार)s” ला अनुमती नाही. अनुमत विस्तार आहेत: "
"%(अनुमत_विस्तार)s."

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "Null characters are not allowed."
msgstr "शून्य वर्णांना अनुमती नाही."

#: env/lib/python3.12/site-packages/django/db/models/base.py
#: env/lib/python3.12/site-packages/django/forms/models.py
#: templates/users/administrator/organization/organization_groups.html
msgid "and"
msgstr "आणि"

#: env/lib/python3.12/site-packages/django/db/models/base.py
#, python-format
msgid "%(model_name)s with this %(field_labels)s already exists."
msgstr "या %(field_labels) सह %(model_name)s आधीपासूनच अस्तित्वात आहेत."

#: env/lib/python3.12/site-packages/django/db/models/constraints.py
#, python-format
msgid "Constraint “%(name)s” is violated."
msgstr "मर्यादा \"%(नाव)s\" चे उल्लंघन केले आहे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid "Value %(value)r is not a valid choice."
msgstr "मूल्य %(value)r ही वैध निवड नाही."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
msgid "This field cannot be null."
msgstr "हे फील्ड शून्य असू शकत नाही."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
msgid "This field cannot be blank."
msgstr "हे फील्ड रिक्त असू शकत नाही."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid "%(model_name)s with this %(field_label)s already exists."
msgstr "या %(field_label) सह %(model_name)s आधीपासून अस्तित्वात आहे."

#. Translators: The 'lookup_type' is one of 'date', 'year' or
#. 'month'. Eg: "Title must be unique for pub_date year"
#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid ""
"%(field_label)s must be unique for %(date_field_label)s %(lookup_type)s."
msgstr ""
"%(field_label)s %(date_field_label)s %(lookup_type)s साठी अद्वितीय असणे "
"आवश्यक आहे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid "Field of type: %(field_type)s"
msgstr "फील्ड प्रकार: %(field_type)s"

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid "“%(value)s” value must be either True or False."
msgstr "“%(मूल्य)s” मूल्य एकतर सत्य किंवा असत्य असणे आवश्यक आहे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid "“%(value)s” value must be either True, False, or None."
msgstr "“%(मूल्य)s” मूल्य एकतर सत्य, असत्य किंवा काहीही नसावे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
msgid "Boolean (Either True or False)"
msgstr "बुलियन (एकतर खरे किंवा खोटे)"

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid "String (up to %(max_length)s)"
msgstr "स्ट्रिंग (%(max_length)s पर्यंत)"

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
msgid "String (unlimited)"
msgstr "स्ट्रिंग (अमर्यादित)"

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
msgid "Comma-separated integers"
msgstr "स्वल्पविरामाने विभक्त पूर्णांक"

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid ""
"“%(value)s” value has an invalid date format. It must be in YYYY-MM-DD "
"format."
msgstr ""
"“%(मूल्य)s” मूल्यामध्ये अवैध तारीख स्वरूप आहे. ते YYYY-MM-DD फॉरमॅटमध्ये "
"असणे आवश्यक आहे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid ""
"“%(value)s” value has the correct format (YYYY-MM-DD) but it is an invalid "
"date."
msgstr ""
"“%(मूल्य)s” मूल्याचे स्वरूप योग्य आहे (YYYY-MM-DD) परंतु ती अवैध तारीख आहे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
msgid "Date (without time)"
msgstr "तारीख (वेळेशिवाय)"

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid ""
"“%(value)s” value has an invalid format. It must be in YYYY-MM-DD "
"HH:MM[:ss[.uuuuuu]][TZ] format."
msgstr ""
"“%(मूल्य)s” मूल्यामध्ये अवैध स्वरूप आहे. ते YYYY-MM-DD "
"HH:MM[:ss[.uuuuuu]][TZ] फॉरमॅटमध्ये असणे आवश्यक आहे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid ""
"“%(value)s” value has the correct format (YYYY-MM-DD "
"HH:MM[:ss[.uuuuuu]][TZ]) but it is an invalid date/time."
msgstr ""
"“%(मूल्य)s” मूल्याचे स्वरूप योग्य आहे (YYYY-MM-DD HH:MM[:ss[.uuuuuu]][TZ]) "
"परंतु ती अवैध तारीख/वेळ आहे."

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
msgid "Date (with time)"
msgstr "तारीख (वेळेसह)"

#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
