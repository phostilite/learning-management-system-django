#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "Enter a valid value."
msgstr "एक मान्य मान दर्ज करें."

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "Enter a valid domain name."
msgstr "एक वैध डोमेन नाम दर्ज करें."

#: env/lib/python3.12/site-packages/django/core/validators.py
#: env/lib/python3.12/site-packages/django/forms/fields.py
msgid "Enter a valid URL."
msgstr "एक मान्य यूआरएल दर्ज करें."

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "Enter a valid integer."
msgstr "एक वैध पूर्णांक दर्ज करें."

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "Enter a valid email address."
msgstr "एक मान्य ईमेल पता दर्ज करें।"

#. Translators: "letters" means latin letters: a-z and A-Z.
#: env/lib/python3.12/site-packages/django/core/validators.py
msgid ""
"Enter a valid “slug” consisting of letters, numbers, underscores or hyphens."
msgstr ""
"अक्षरों, संख्याओं, अंडरस्कोर या हाइफ़न से युक्त एक वैध \"स्लग\" दर्ज करें।"

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid ""
"Enter a valid “slug” consisting of Unicode letters, numbers, underscores, or"
" hyphens."
msgstr ""
"यूनिकोड अक्षरों, संख्याओं, अंडरस्कोर या हाइफ़न से युक्त एक वैध \"स्लग\" दर्ज"
" करें।"

#: env/lib/python3.12/site-packages/django/core/validators.py
#: env/lib/python3.12/site-packages/django/db/models/fields/__init__.py
#, python-format
msgid "Enter a valid %(protocol)s address."
msgstr "एक वैध %(प्रोटोकॉल) पता दर्ज करें."

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "IPv4"
msgstr "आईपीवीसीएच"

#: env/lib/python3.12/site-packages/django/core/validators.py
#: env/lib/python3.12/site-packages/django/utils/ipv6.py
msgid "IPv6"
msgstr "आईपीवीएसएच"

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "IPv4 or IPv6"
msgstr "आईपीवी4 या आईपीवी6"

#: env/lib/python3.12/site-packages/django/core/validators.py
msgid "Enter only digits separated by commas."
msgstr "केवल अल्पविराम से अलग किए गए अंक दर्ज करें।"

#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid "Ensure this value is %(limit_value)s (it is %(show_value)s)."
msgstr "सुनिश्चित करें कि यह मान %(limit_value)s है (यह %(show_value)s है)।"

#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid "Ensure this value is less than or equal to %(limit_value)s."
msgstr "सुनिश्चित करें कि यह मान %(limit_value)s से कम या उसके बराबर है।"

#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid "Ensure this value is greater than or equal to %(limit_value)s."
msgstr "सुनिश्चित करें कि यह मान %(limit_value)s से अधिक या उसके बराबर है।"

#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid "Ensure this value is a multiple of step size %(limit_value)s."
msgstr "सुनिश्चित करें कि यह मान चरण आकार %(limit_value)s का गुणज है।"

#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid ""
"Ensure this value is a multiple of step size %(limit_value)s, starting from "
"%(offset)s, e.g. %(offset)s, %(valid_value1)s, %(valid_value2)s, and so on."
msgstr ""
"सुनिश्चित करें कि यह मान चरण आकार %(limit_value)s का गुणज है, जो %(offset)s "
"से शुरू होता है, उदाहरण के लिए %(ऑफसेट)s, %(valid_value1)s, "
"%(valid_value2)s, इत्यादि।"

#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid ""
"Ensure this value has at least %(limit_value)d character (it has "
"%(show_value)d)."
msgid_plural ""
"Ensure this value has at least %(limit_value)d characters (it has "
"%(show_value)d)."
msgstr[0] ""
msgstr[1] ""

#: env/lib/python3.12/site-packages/django/core/validators.py
#, python-format
msgid ""
"Ensure this value has at most %(limit_value)d character (it has "
"%(show_value)d)."
msgid_plural ""
"Ensure this value has at most %(limit_value)d characters (it has "
"%(show_value)d)."
msgstr[0] ""
msgstr[1] ""

